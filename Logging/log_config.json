{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(name)s:%(lineno)s %(funcName)s [%(levelname)s]: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "__main__": {
            "level": "DEBUG",
            "handlers": [ "consoleHandler" ],
            "propagate": false
        },
        "same_hierarchy": {
            "level": "DEBUG",
            "handlers": [ "consoleHandler" ],
            "propagate": false
        },
        "lower.sub": {
            "level": "DEBUG",
            "handlers": [ "consoleHandler" ],
            "propagate": false
        },
        "DD_FBX_Importer": {
            "level": "DEBUG",
            "handlers": [ "consoleHandler" ],
            "propagate": false
        }
    },
    "root": {
        "level": "INFO"
    }
}
